  <script>
    window.onload = function() {
      // Get the SVG element
      const svg = document.querySelector('.links-layer svg');
      
      // Create a group for orbital dots at the end of the SVG (so they're on top)
      const orbitalDotsGroup = document.createElementNS('http://www.w3.org/2000/svg', 'g');
      orbitalDotsGroup.setAttribute('id', 'orbital-dots-group');
      svg.appendChild(orbitalDotsGroup);
      
      // Make the rect elements transparent but still clickable
      const svgRects = document.querySelectorAll('.links-layer svg a rect');
      svgRects.forEach(rect => {
        rect.setAttribute('fill', 'rgba(255, 255, 255, 0)');
        rect.setAttribute('stroke', 'transparent');
        
        // Add orbital dots hover effect
        const linkElement = rect.parentElement;
        addOrbitalDotsEffect(linkElement, rect, orbitalDotsGroup);
      });
    };
    
    // Function to add orbital dots hover effect
    function addOrbitalDotsEffect(linkElement, rect, dotsContainer) {
      // Get dimensions of the rectangle for positioning
      const rectWidth = parseFloat(rect.getAttribute('width'));
      const rectHeight = parseFloat(rect.getAttribute('height'));
      const rectX = parseFloat(rect.getAttribute('x'));
      const rectY = parseFloat(rect.getAttribute('y'));
      
      // Calculate center position
      const cx = rectX + (rectWidth / 2);
      const cy = rectY + (rectHeight / 2);
      
      // Calculate radius for orbit (10px larger than icon)
      const iconSize = Math.min(rectWidth, rectHeight) / 2;
      const orbitRadius = iconSize + 10;
      
      // Create an orbit group for this specific icon
      const orbitGroup = document.createElementNS('http://www.w3.org/2000/svg', 'g');
      orbitGroup.setAttribute('class', 'orbit-group');
      orbitGroup.style.display = 'none'; // Initially hidden
      dotsContainer.appendChild(orbitGroup);
      
      // Store animation info
      let animationFrameId = null;
      let dotInfos = [];
      
      // Create the 3 orbital dots in advance
      for (let i = 0; i < 3; i++) {
        // Create dot
        const dot = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
        dot.setAttribute('r', 3); // 6px diameter (increased from 4px)
        
        // Alternate between seafoam green and violet colors
        const colors = ['#7FFFD4', '#EE82EE']; // Seafoam green and violet
        const colorIndex = i % 2; // Alternates between 0 and 1
        const dotColor = colors[colorIndex];
        
        // Increased opacity to 0.8
        dot.setAttribute('fill', dotColor);
        dot.setAttribute('opacity', '0.8');
        
        // Stronger glow effect
        dot.setAttribute('filter', 'drop-shadow(0 0 4px ' + dotColor + ') drop-shadow(0 0 2px rgba(255, 255, 255, 0.6))');
        
        // Add to the orbit group
        orbitGroup.appendChild(dot);
        
        // Random starting angle
        const startAngle = Math.random() * Math.PI * 2;
        
        // Random orbit speed (1.8s-2.5s per orbit)
        const orbitalPeriod = (1800 + Math.random() * 700); // ms
        const angularVelocity = (Math.PI * 2) / orbitalPeriod; // radians per ms
        
        // Store dot info
        dotInfos.push({
          element: dot,
          angle: startAngle,
          velocity: angularVelocity,
          color: dotColor
        });
        
        // Initial position
        updateDotPosition(dotInfos[i], cx, cy, orbitRadius);
      }
      
      // Function to update dot positions
      function updateDotsPositions(timestamp) {
        // Calculate elapsed time
        const elapsed = timestamp - lastTimestamp;
        lastTimestamp = timestamp;
        
        // Update each dot position
        for (const dotInfo of dotInfos) {
          // Update angle based on angular velocity and elapsed time
          dotInfo.angle += dotInfo.velocity * elapsed;
          
          // Normalize angle to keep it in the 0-2π range
          if (dotInfo.angle > Math.PI * 2) {
            dotInfo.angle -= Math.PI * 2;
          }
          
          // Update position
          updateDotPosition(dotInfo, cx, cy, orbitRadius);
        }
        
        // Continue animation
        animationFrameId = requestAnimationFrame(updateDotsPositions);
      }
      
      // Helper function to update a dot's position
      function updateDotPosition(dotInfo, centerX, centerY, radius) {
        const x = centerX + Math.cos(dotInfo.angle) * radius;
        const y = centerY + Math.sin(dotInfo.angle) * radius;
        dotInfo.element.setAttribute('cx', x);
        dotInfo.element.setAttribute('cy', y);
      }
      
      let lastTimestamp;
      
      // Add mouseenter event
      linkElement.addEventListener('mouseenter', () => {
        // Show the orbit group
        orbitGroup.style.display = 'block';
        orbitGroup.style.opacity = '1';
        
        // Start animation loop
        lastTimestamp = performance.now();
        animationFrameId = requestAnimationFrame(updateDotsPositions);
      });
      
      // Prevent instant navigation to allow seeing the hover effect
      linkElement.addEventListener('click', (event) => {
        // Prevent default navigation
        event.preventDefault();
        
        // Store the href to navigate to
        const href = linkElement.getAttribute('xlink:href');
        
        // Wait 1 second to show the effect, then navigate
        setTimeout(() => {
          window.location.href = href;
        }, 1000);
      });
      
      // Add mouseleave event
      linkElement.addEventListener('mouseleave', () => {
        // Stop animation
        if (animationFrameId) {
          cancelAnimationFrame(animationFrameId);
          animationFrameId = null;
        }
        
        // Fade out orbit group
        orbitGroup.style.transition = 'opacity 0.5s ease-out';
        orbitGroup.style.opacity = '0';
        
        // Hide after fade out completes
        setTimeout(() => {
          orbitGroup.style.display = 'none';
        }, 500);
      });
    }
  </script>

