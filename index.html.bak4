  <script>
    // Bottom Sheet Functionality
    document.addEventListener('DOMContentLoaded', function() {
      const bottomSheet = document.getElementById('bottom-sheet');
      const toggleButton = document.getElementById('toggle-button');
      
      // Force consistent height
      bottomSheet.style.height = '160px';
      
      toggleButton.addEventListener('click', function() {
        bottomSheet.classList.toggle('receded');
        // Ensure height remains consistent even after toggling
        bottomSheet.style.height = '160px';
      });
      
      // Add resize handler to maintain consistent sheet size
      window.addEventListener('resize', function() {
        bottomSheet.style.height = '160px';
      });
      
      // Variables to track which links are inserted
      let hasChart = false;
      let hasWebsite = false;
      let hasX = false;
      
      // Handle capsule buttons (Insert Chart, Insert Website, Insert X)
      const capsuleButtons = document.querySelectorAll('.capsule-button');
      capsuleButtons.forEach(button => {
        button.addEventListener('click', function() {
          const buttonText = this.textContent.trim();
          if (buttonText === 'Insert Chart') {
            hasChart = true;
          } else if (buttonText === 'Insert Website') {
            hasWebsite = true;
          } else if (buttonText === 'Insert X') {
            hasX = true;
          }
          // Visual feedback for the button click
          this.style.backgroundColor = '#e0e0e0';
          setTimeout(() => {
            this.style.backgroundColor = '';
          }, 200);
        });
      });
      
      // Handle emoji clicks
      const emojis = document.querySelectorAll('.emoji');
      emojis.forEach(emoji => {
        emoji.addEventListener('click', function() {
          const shillMessage = document.getElementById('shill-message');
          shillMessage.value += ' ' + this.textContent;
        });
      });
      
      // Handle "Update Links" button click
      const updateButton = document.getElementById('update-button');
      updateButton.addEventListener('click', function() {
        updateAllLinks();
      });
      
      // Function to update all social media links
      function updateAllLinks() {
        // Get the message from the textarea
        const inputMessage = document.getElementById('shill-message').value.trim();
        
        // Build the full message with optional links
        let fullMessage = inputMessage;
        
        // Add selected emojis
        const selectedEmojis = Array.from(document.querySelectorAll('.emoji'))
          .map(emoji => emoji.textContent)
          .join(' ');
        
        if (selectedEmojis) {
          fullMessage += ' ' + selectedEmojis;
        }
        
        // Add inserted links if selected
        if (hasChart) {
          fullMessage += ' https://dexscreener.com/';
        }
        
        if (hasWebsite) {
          fullMessage += ' https://easyshill.com/';
        }
        
        if (hasX) {
          fullMessage += ' https://x.com/easy_shill';
        }
        
        // URL encode the message for use in links
        const encodedMessage = encodeURIComponent(fullMessage);
        
        // Update all social media links in the image map
        const mapAreas = document.querySelectorAll('map[name="ring-map"] area');
        
        mapAreas.forEach(area => {
          const type = area.getAttribute('alt');
          let newHref = '';
          
          // Update each link based on its social media type
          switch(type) {
            case 'pocket':
              newHref = `https://getpocket.com/edit?url=https%3A//easyshill.com/%23how-to-buy&title=${encodedMessage}`;
              break;
            case 'pinterest':
              newHref = `https://pinterest.com/pin/create/button/?url=https%3A//easyshill.com/%23how-to-buy&description=${encodedMessage}`;
              break;
            case 'xing':
              newHref = `https://www.xing.com/spi/shares/new?url=https%3A//easyshill.com/%23how-to-buy&text=${encodedMessage}`;
              break;
            case 'messenger':
            case 'messenger2':
              newHref = `https://www.facebook.com/dialog/send?link=https%3A//easyshill.com/%23how-to-buy&app_id=123456789&redirect_uri=https%3A//easyshill.com/%23how-to-buy&text=${encodedMessage}`;
              break;
            case 'mastodon':
              newHref = `https://mastodon.social/share?text=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'whatsapp':
              newHref = `https://api.whatsapp.com/send?text=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'flipboard':
              newHref = `https://share.flipboard.com/bookmarklet/popout?v=2&title=${encodedMessage}&url=https%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'linkedin':
              newHref = `https://www.linkedin.com/sharing/share-offsite/?url=https%3A//easyshill.com/%23how-to-buy&summary=${encodedMessage}`;
              break;
            case 'sms':
              newHref = `sms:?body=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'vk':
              newHref = `https://vk.com/share.php?url=https%3A//easyshill.com/%23how-to-buy&title=${encodedMessage}`;
              break;
            case 'skype':
              newHref = `https://web.skype.com/share?url=https%3A//easyshill.com/%23how-to-buy&text=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'line':
              newHref = `https://social-plugins.line.me/lineit/share?url=https%3A//easyshill.com/%23how-to-buy&text=${encodedMessage}`;
              break;
            case 'reddit':
              newHref = `https://www.reddit.com/submit?url=https%3A//easyshill.com/%23how-to-buy&title=${encodedMessage}`;
              break;
            case 'gab':
              newHref = `https://gab.com/compose?text=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'twitter':
              newHref = `https://twitter.com/intent/tweet?text=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'email':
              newHref = `mailto:?subject=${encodedMessage}&body=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            case 'facebook':
              newHref = `https://www.facebook.com/sharer/sharer.php?u=https%3A//easyshill.com/%23how-to-buy&quote=${encodedMessage}`;
              break;
            case 'tumblr':
              newHref = `https://www.tumblr.com/widgets/share/tool?canonicalUrl=https%3A//easyshill.com/%23how-to-buy&caption=${encodedMessage}`;
              break;
            case 'hackernews':
              newHref = `https://news.ycombinator.com/submitlink?u=https%3A//easyshill.com/%23how-to-buy&t=${encodedMessage}`;
              break;
            case 'tiktok':
              newHref = `https://www.tiktok.com/upload?text=${encodedMessage}`;
              break;
            case 'weibo':
              newHref = `https://service.weibo.com/share/share.php?url=https%3A//easyshill.com/%23how-to-buy&title=${encodedMessage}`;
              break;
            case 'custom':
              newHref = `https%3A//easyshill.com/%23how-to-buy&text=${encodedMessage}%0Ahttps%3A//easyshill.com/%23how-to-buy`;
              break;
            default:
              // Keep the original href if type not recognized
              newHref = area.getAttribute('href');
          }
          
          // Update the href attribute
          area.setAttribute('href', newHref);
        });
        
        // Also update SVG links for consistency
        const svgLinks = document.querySelectorAll('.links-layer svg a');
        svgLinks.forEach(link => {
          // Get the corresponding href from the map area with the same index
          const index = Array.from(svgLinks).indexOf(link);
          if (index < mapAreas.length) {
            const newHref = mapAreas[index].getAttribute('href');
            link.setAttribute('xlink:href', newHref);
          }
        });
        
        // Visual feedback: Flash green light
        const redLight = document.getElementById('red-light-copy');
        const greenLight = document.getElementById('green-light-copy');
        
        redLight.classList.remove('active');
        greenLight.classList.add('active');
        
        // Return to red after 3 seconds
        setTimeout(() => {
          greenLight.classList.remove('active');
          redLight.classList.add('active');
        }, 3000);
      }
    });
  </script>
  
